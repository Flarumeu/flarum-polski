{
    "name": "flarumeu/flarum-polski",
    "type": "flarum-extension",
    "description": "Polska paczka jezykowa.",
    "homepage": "https://github.com/flarumeu/flarum-polski",
    "keywords": ["locale"],
    "license": "MIT",
    "require": {
        "flarum/core": "^0.1.0-beta.7"
    },
	
	"authors": [
        {
            "name": "Leon"
        }
	],
	
	"support": {
        "issues": "https://github.com/flarumeu/flarum-polski/issues",
        "source": "https://github.com/flarumeu/flarum-polski"
				}
	
    "extra": {
